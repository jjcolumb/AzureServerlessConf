@inherits LayoutComponentBase
<nav class="navbar navbar-light navbar-expand-lg bg-white border-bottom shadow-sm">
    <a class="navbar-brand" href="#">
        <h5 class="font-weight-normal">
            <kbd>Azure Serverless Conf Speakers</kbd>
        </h5>
    </a>


    <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav mr-auto"></ul>
        <ul class="navbar-nav">
            <li class="nav-item active">
                <a class="nav-link text-dark font-weight-bold" href="https://github.com/jjcolumb/AzureServerlessConf" target="_blank"><i class="fab fa-lg fa-github"></i> About</a>
            </li>

        </ul>
    </div>
</nav>
<div class="container">
    <div class="px-3 py-3 pt-md-5 pb-md-4 mx-auto">
        <h3 class="text-muted">Azure Serverless Conf Speakers - September 2021</h3>
        <p class="lead text-monospace"><i class="em em-heart"></i> Free, virtual developer event focused on building event-driven apps using Azure Serverless technologies. These technologies include Functions, Cosmos DB, Azure SQL DB, Logic Apps and more. </p>
    </div>
    @Body
</div>

<div class="container">
    <footer class="pt-4 my-md-5 pt-md-5 border-top">
        <div class="row">
            <div class="col-12 col-md">
                <h5 class="font-weight-normal">
                    <kbd>Azure Serverless Conf</kbd>
                </h5>
                <small class="d-block mb-3 text-muted">&copy; 2021</small>
            </div>
        </div>
    </footer>
</div>
